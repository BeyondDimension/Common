<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net$(DotNet_Version)</TargetFrameworks>
		<TargetFrameworks Condition="$([MSBuild]::IsOSPlatform('windows'))">net$(DotNet_Version);net$(DotNet_Version)-windows$(WinSDK_Version)</TargetFrameworks>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<IsPackable>false</IsPackable>
		<IsTestProject>true</IsTestProject>
		<UserSecretsId>3ca41323-7e26-4b3e-8730-2e839c58c39c</UserSecretsId>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\BD.Common8.Security\Helpers\SecurityPlatformHelper.MacOS.cs">
			<LinkBase>_SourceReference\Helpers</LinkBase>
		</Compile>
	</ItemGroup>
	
	<ItemGroup Condition="Exists('..\..\..\Constants.Sms.cs')">
		<Compile Remove="Constants.Sms.cs" />
		<Compile Include="..\..\..\Constants.Sms.cs" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="MonoMac.NetStandard" />
		<PackageReference Include="Microsoft.IdentityModel.Tokens" />
	</ItemGroup>

	<ItemGroup>
		<Using Include="NUnit.Framework" />
		<Using Include="NUnit.Framework.Interfaces" />
		<Using Include="NUnit.Framework.Internal" />
		<Using Include="NUnit.Framework.Internal.Commands" />
		<Using Include="BD.Common8.UnitTest" />
		<Using Include="BD.Common8.UnitTest.Models" />
	</ItemGroup>

	<Import Project="$(MSBuildThisFileDirectory)..\Sdk\BD.Common8.UnitTest.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\Shared\GlobalUsings.Microsoft.IdentityModel.Tokens.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\Sdk\ProjectReference\BD.Common8.Bcl.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\Sdk\ProjectReference\BD.Common8.Pinyin.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\Sdk\ProjectReference\BD.Common8.Pinyin.ChnCharInfo.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\Sdk\ProjectReference\BD.Common8.Primitives.District.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\Sdk\ProjectReference\BD.Common8.Security.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\Sdk\ProjectReference\BD.Common8.SmsSender.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\Sdk\ProjectReference\BD.Common8.HuaweiCloud.SDK.Obs.props" />
</Project>
