	</ItemGroup>

	<Import Project="$(MSBuildThisFileDirectory)..\Shared\GlobalUsings.BD.Common8.UnitTest.props" />

	<ItemGroup >
		<None Include="$(MSBuildThisFileDirectory)..\Shared\GlobalUsings.BD.Common8.UnitTest.props">
