<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net$(DotNet_Version)</TargetFrameworks>
		<UseCommon8Bcl>true</UseCommon8Bcl>
		<UseCommon8Essentials>true</UseCommon8Essentials>
		<UseCommon8RepositoriesEFCore>true</UseCommon8RepositoriesEFCore>
		<UseCommon8Primitives>true</UseCommon8Primitives>
		<UseAspNetCore>true</UseAspNetCore>
		<UseEFCore>true</UseEFCore>
		<UseMicrosoftIdentityModelTokens>true</UseMicrosoftIdentityModelTokens>
		<UseRedis>true</UseRedis>
		<UseNLog>true</UseNLog>
		<IsGlobalUsingsMSBuildProjectName>true</IsGlobalUsingsMSBuildProjectName>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	
</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)'=='Release'">
		<IsPackable>true</IsPackable>
	</PropertyGroup>

</Project>
