<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Product>次元超越拼音 TinyPinyin 实现库</Product>
		<PackageTags>BeyondDimension Common CommonPinyinTinyPinyin Pinyin TinyPinyin</PackageTags>
		<Description>$(Product)</Description>
		<RootNamespace>BD.Common</RootNamespace>
		<DefineConstants>__NOT_IMPORT_COLUMNS__;__NOT_IMPORT_MODELS__;$(DefineConstants)</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
	</ItemGroup>

	<ItemGroup>
		<Compile Include="..\ImplicitUsings.Primitives.cs">
			<LinkBase>Properties</LinkBase>
		</Compile>
		<Compile Include="..\ImplicitUsings.Services.cs">
			<LinkBase>Properties</LinkBase>
		</Compile>
	</ItemGroup>

	<ItemGroup Condition=" $(TargetFramework.StartsWith('MonoAndroid')) Or $(TargetFramework.Contains('-android')) ">
		<PackageReference Include="XAB.TinyPinyin" />
	</ItemGroup>

	<ItemGroup Condition=" !($(TargetFramework.StartsWith('MonoAndroid')) Or $(TargetFramework.Contains('-android'))) ">
		<PackageReference Include="TinyPinyin.Net" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\BD.Common.Pinyin\BD.Common.Pinyin.csproj" />
		<ProjectReference Include="..\BD.Common\BD.Common.csproj" />
	</ItemGroup>

	<Import Project="..\TFM_NETX_WITH_ANDROID.props" />
	<Import Project="..\GeneratePackage.props" />

</Project>
