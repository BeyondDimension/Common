<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net$(DotNet_Version)</TargetFramework>
		<InvariantGlobalization>true</InvariantGlobalization>
		<PublishAot>false</PublishAot>
		<UseCommon8Bcl>true</UseCommon8Bcl>
		<UseCommon8Ipc>true</UseCommon8Ipc>
		<UseCommon8IpcServer>true</UseCommon8IpcServer>
		<UseCommon8PrimitivesApiRsp>true</UseCommon8PrimitivesApiRsp>
		<UseProjectUtils>true</UseProjectUtils>
		<UseAspNetCore>true</UseAspNetCore>
		<UseSwagger>true</UseSwagger>
		<UseSourceGeneratorIpcServer>true</UseSourceGeneratorIpcServer>
	</PropertyGroup>

</Project>
