<Project>
	<!-- 📄 .NET Framework 3.5 SDK 兼容 -->

	<!-- 👇 属性配置 -->
	<PropertyGroup>
		<!-- ImplicitUsings 会导致引入的命名空间包含不存在的命名空间 -->
		<ImplicitUsings>false</ImplicitUsings>
	</PropertyGroup>

	<!-- 👇 全局 using | Global usings -->
	<ItemGroup>
		<Compile Include="$(MSBuildThisFileDirectory)..\Shared\GlobalUsings.net35.cs">
			<LinkBase>Properties\GlobalUsings</LinkBase>
		</Compile>
	</ItemGroup>
</Project>