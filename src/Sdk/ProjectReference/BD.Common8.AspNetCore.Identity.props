<Project>
	<!-- 项目引用 -->
	<Import Condition="Exists('$(MSBuildThisFileDirectory)..\..\Shared\GlobalUsings.$(MSBuildThisFileName).props')" Project="$(MSBuildThisFileDirectory)..\..\Shared\GlobalUsings.$(MSBuildThisFileName).props" />
	<Import Condition="Exists('$(MSBuildThisFileDirectory)..\$(MSBuildThisFileName)\GlobalUsings.props')" Project="$(MSBuildThisFileDirectory)..\$(MSBuildThisFileName)\GlobalUsings.props" />
	<ImportGroup Condition="$(MSBuildProjectName) == $(MSBuildThisFileName)">
		<Import Condition="Exists('$(MSBuildThisFileDirectory)..\$(MSBuildThisFileName)\Reference.props')" Project="$(MSBuildThisFileDirectory)..\$(MSBuildThisFileName)\Reference.props" />
		<Import Condition="Exists('$(MSBuildThisFileDirectory)..\..\..\res\i18n\$(MSBuildThisFileName).props')" Project="$(MSBuildThisFileDirectory)..\..\..\res\i18n\$(MSBuildThisFileName).props" />
	</ImportGroup>
	<ItemGroup Condition="Exists('$(MSBuildThisFileDirectory)..\..\$(MSBuildThisFileName)\$(MSBuildThisFileName).csproj') AND $(MSBuildProjectName) != $(MSBuildThisFileName)">
		<ProjectReference Include="$(MSBuildThisFileDirectory)..\..\$(MSBuildThisFileName)\$(MSBuildThisFileName).csproj" />
	</ItemGroup>
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.SourceGenerator.ResX.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.Bcl.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.Primitives.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.Primitives.Essentials.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.Orm.EFCore.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.Repositories.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.Repositories.EFCore.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.AspNetCore.props" />
</Project>