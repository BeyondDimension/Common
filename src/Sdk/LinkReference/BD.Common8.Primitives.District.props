<Project>
	<!-- 源码引用 -->
	<Import Condition="Exists('$(MSBuildThisFileDirectory)..\..\Shared\GlobalUsings.$(MSBuildThisFileName).props')" Project="$(MSBuildThisFileDirectory)..\..\Shared\GlobalUsings.$(MSBuildThisFileName).props" />
	<Import Condition="Exists('$(MSBuildThisFileDirectory)..\$(MSBuildThisFileName)\GlobalUsings.props')" Project="$(MSBuildThisFileDirectory)..\$(MSBuildThisFileName)\GlobalUsings.props" />
	<Import Condition="Exists('$(MSBuildThisFileDirectory)..\$(MSBuildThisFileName)\Reference.props')" Project="$(MSBuildThisFileDirectory)..\$(MSBuildThisFileName)\Reference.props" />
	<Import Condition="Exists('$(MSBuildThisFileDirectory)..\..\..\res\i18n\$(MSBuildThisFileName).props')" Project="$(MSBuildThisFileDirectory)..\..\..\res\i18n\$(MSBuildThisFileName).props" />
	<ItemGroup>
		<Compile Include="$(MSBuildThisFileDirectory)..\..\$(MSBuildThisFileName)\**\*.cs">
			<LinkBase>_SourceReference\$(MSBuildThisFileName)</LinkBase>
		</Compile>
		<Compile Remove="$(MSBuildThisFileDirectory)..\..\$(MSBuildThisFileName)\bin\**" />
		<Compile Remove="$(MSBuildThisFileDirectory)..\..\$(MSBuildThisFileName)\obj\**" />
	</ItemGroup>

	<ItemGroup Condition="$(MSBuildProjectName) == $(MSBuildThisFileName)">
		<EmbeddedResource Include="$(MSBuildThisFileDirectory)..\..\..\res\AMap_adcode_citycode.resx">
			<LinkBase>_SourceReference\BD.Common8.Primitives.District\Properties</LinkBase>
			<Generator></Generator>
			<LastGenOutput></LastGenOutput>
			<LogicalName>FxResources.BD.Common8.Primitives.District.Properties.Resources.resources</LogicalName>
			<SubType>Designer</SubType>
		</EmbeddedResource>
	</ItemGroup>
	<Import Project="$(MSBuildThisFileDirectory)..\ProjectReference\BD.Common8.SourceGenerator.ResX.props" />
	<Import Project="$(MSBuildThisFileDirectory)..\GlobalUsings\BD.Common8.Bcl.props" />
</Project>