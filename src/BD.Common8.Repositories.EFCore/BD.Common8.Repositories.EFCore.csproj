<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net$(DotNet_Version)</TargetFramework>
		<UseCommon8Bcl>true</UseCommon8Bcl>
		<UseEFCore>true</UseEFCore>
		<UseCommon8Primitives>true</UseCommon8Primitives>
		<UseCommon8OrmEFCore>true</UseCommon8OrmEFCore>
		<UseCommon8Repositories>true</UseCommon8Repositories>
		<IsGlobalUsingsMSBuildProjectName>true</IsGlobalUsingsMSBuildProjectName>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)'=='Release'">
		<IsPackable>true</IsPackable>
	</PropertyGroup>

</Project>
