{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/artifacts/bin/**/BD.Common8*.dll"
          ],
          "exclude": [
            "src/artifacts/bin/**/*.resources.dll"
          ]
        }
      ],
      "dest": "api",
      "outputFormat": "mref",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "res/DocFXIcon.png"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "BD.Common8 文档",
      "_appName": "Common8",
      "_appLogoPath": "res/DocFXIcon.png",
      "_appLogoUrl": "https://github.com/BeyondDimension/Common",
      "_appFooter": "©️ 江苏蒸汽凡星科技有限公司. All rights reserved."
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}